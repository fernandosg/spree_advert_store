<div data-hook="admin_image_form_fields" class="row">
  <div class="col-xs-12 col-md-6">
    <div data-hook="code" class="form-group">
      <%= f.label :code, Spree.t(:code) %>
      <%= f.text_field :code %>
    </div>
    <div data-hook="type_advertisement" class="form-group">
      <%= f.label :type_advertisement, Spree.t(:type_advertisement) %>
      <%= f.text_field :type_advertisement %>
    </div>
    <div data-hook="advrt_rf_id" class="form-group">
      <%= f.label :advrt_rf_id, Spree.t(:advrt_rf_id) %>
      <%= f.text_field :advrt_rf_id %>
    </div>
  </div>
</div>
